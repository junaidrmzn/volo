{
    "name": "voloiq-ui",
    "packageManager": "yarn@3.1.1",
    "workspaces": [
        "packages/**/*",
        "scripts/generator"
    ],
    "bin": {
        "api-proxy": "./scripts/proxy.js",
        "run-with-since": "./scripts/runWithSince.js"
    },
    "scripts": {
        "setup": "yarn install && yarn generate:code && yarn preconstruct && lerna run build --scope @voloiq/table --stream",
        "preconstruct": "preconstruct dev",
        "prelint": "eslint \"./{config,scripts}/**/*.{js,ts,tsx}\" \".eslintrc.js\"",
        "lint": "run-with-since lerna run lint && yarn npmPkgJsonLint .",
        "prelint:fix": "yarn prelint --fix",
        "lint:fix": "yarn prelint:fix && lerna run lint:fix",
        "lint:staged": "yarn lint-staged --verbose",
        "typecheck": "run-with-since lerna run typecheck --reject-cycles --concurrency 1",
        "test:pact": "lerna run test:pact --stream --concurrency 1",
        "test": "run-with-since lerna run test --stream --concurrency 1 && ts-node ./scripts/jest/mergeCoverageReports.ts",
        "clean": "lerna clean -y && lerna run clean && rimraf ./coverage ./node_modules",
        "generate:package": "lerna run generate --scope @voloiq/generator --stream --",
        "generate:code": "lerna run generate:code --stream",
        "start:voloiq": "lerna run start --scope @voloiq/app --stream",
        "start:voloiq:proxy": "api-proxy",
        "start:voloiq:withMocks": "lerna run start:withMocks --scope @voloiq/app --stream",
        "build:voloiq": "lerna run build --scope @voloiq/app --stream",
        "storybook": "cross-env NODE_ENV=development start-storybook -p 6006"
    },
    "devDependencies": {
        "@babel/core": "7.17.0",
        "@babel/preset-env": "7.16.11",
        "@babel/preset-react": "7.16.7",
        "@babel/preset-typescript": "7.16.7",
        "@jest/types": "27.4.2",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
        "@preconstruct/cli": "2.1.5",
        "@statoscope/webpack-plugin": "5.20.1",
        "@storybook/addon-a11y": "6.4.18",
        "@storybook/addon-actions": "6.4.18",
        "@storybook/addon-essentials": "6.4.18",
        "@storybook/addon-links": "6.4.18",
        "@storybook/builder-webpack5": "next",
        "@storybook/manager-webpack5": "next",
        "@storybook/react": "6.4.18",
        "@svgr/webpack": "6.2.1",
        "@testing-library/jest-dom": "5.16.5",
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/fs-extra": "9.0.13",
        "@types/jest": "27.4.0",
        "@types/jest-axe": "3.5.4",
        "@types/node": "16.11.22",
        "@types/webpack": "5.28.0",
        "@types/webpack-bundle-analyzer": "4.4.1",
        "@types/webpack-dev-server": "4.7.2",
        "@types/webpack-env": "1.16.3",
        "@typescript-eslint/eslint-plugin": "5.10.2",
        "@typescript-eslint/parser": "5.10.2",
        "@volocopter/eslint-config": "0.1.0",
        "babel-jest": "27.4.6",
        "babel-loader": "8.2.3",
        "babel-plugin-inline-react-svg": "2.0.1",
        "concurrently": "7.0.0",
        "copy-webpack-plugin": "10.2.4",
        "cross-env": "7.0.3",
        "css-loader": "6.6.0",
        "dotenv": "16.0.0",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-airbnb-typescript": "16.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-deprecation": "2.0.0",
        "eslint-plugin-destructuring": "2.2.1",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-no-only-tests": "3.0.0",
        "eslint-plugin-no-type-assertion": "1.1.1",
        "eslint-plugin-prefer-type-alias": "0.1.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.3.0",
        "eslint-plugin-testing-library": "5.5.1",
        "eslint-plugin-unicorn": "40.1.0",
        "eslint-plugin-use-encapsulation": "1.0.0",
        "fs-extra": "10.0.0",
        "html-webpack-plugin": "5.5.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "jest-axe": "5.0.1",
        "lerna": "4.0.0",
        "lint-staged": "12.3.7",
        "msw": "0.47.0",
        "npm-package-json-lint": "6.4.0",
        "nyc": "15.1.0",
        "prettier": "2.6.0",
        "rimraf": "3.0.2",
        "style-loader": "3.3.1",
        "terser-webpack-plugin": "5.3.1",
        "ts-loader": "9.2.6",
        "ts-node": "10.4.0",
        "typescript": "4.5.5",
        "webpack": "5.68.0",
        "webpack-bundle-analyzer": "4.5.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.9.3",
        "webpack-merge": "5.8.0"
    },
    "resolutions": {
        "@chakra-ui/system": "1.10.1",
        "@chakra-ui/theme-tools": "1.3.5",
        "@chakra-ui/utils": "1.10.1"
    },
    "preconstruct": {
        "packages": [
            "packages/libs/utils",
            "packages/libs/table"
        ]
    }
}
