{
    "name": "@voloiq/generator",
    "version": "1.0.0",
    "main": "src/index.ts",
    "scripts": {
        "clean": "rimraf ./dist ./build-statistics ./coverage",
        "lint": "eslint \"**/*.ts\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "test": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "generate": "ts-node src/index.ts"
    },
    "devDependencies": {
        "@types/mock-fs": "4.13.1",
        "commander": "8.3.0",
        "cross-env": "7.0.3",
        "eslint": "7.32.0",
        "fs-extra": "10.0.0",
        "jest": "27.4.7",
        "mock-fs": "5.1.2",
        "rimraf": "3.0.2",
        "ts-node": "10.4.0",
        "typescript": "4.5.5"
    }
}
