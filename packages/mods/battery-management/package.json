{
    "name": "@voloiq/battery-management",
    "version": "1.0.0",
    "main": "src/index.tsx",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.development.ts",
        "start:withMocks": "cross-env NODE_ENV=development WITH_MOCKS=true webpack serve --config ./config/webpack.development.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpack.production.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage ./src/api",
        "test": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts --passWithNoTests",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "cypress:open": "cypress open --config-file config/cypress.config.ts",
        "cypress:run": "cypress run --config-file config/cypress.config.ts",
        "cypress:verify": "cypress verify"
    },
    "dependencies": {
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@testing-library/react-hooks": "8.0.1",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/feature-flags": "1.0.0",
        "@voloiq/form": "1.0.0",
        "@voloiq/i18n": "1.0.0",
        "@voloiq/resource-overview": "1.0.0",
        "@voloiq/routing": "1.0.0",
        "@voloiq/service": "1.0.0",
        "@voloiq/table": "1.0.0",
        "@voloiq/text-layouts": "1.0.0",
        "@voloiq/utils": "1.0.0",
        "framer-motion": "5.6.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-use": "17.3.2",
        "ts-pattern": "3.3.5"
    },
    "devDependencies": {
        "@mswjs/data": "0.8.4",
        "@pact-foundation/pact": "9.17.3",
        "@testing-library/cypress": "9.0.0",
        "@types/faker": "5.5.3",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@voloiq-typescript-api/battery-management-reports-types": "1.0.3",
        "@voloiq-typescript-api/battery-management-types": "4.1.3",
        "@voloiq/testing": "1.0.0",
        "cross-env": "7.0.3",
        "cypress": "12.3.0",
        "date-fns": "2.28.0",
        "eslint": "7.32.0",
        "faker": "5.5.3",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "jest-pact": "0.9.4",
        "msw": "0.36.8",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "ts-node": "10.8.2",
        "typescript": "4.5.5",
        "webpack": "5.68.0",
        "webpack-cli": "5.1.4"
    },
    "msw": {
        "workerDirectory": "resources"
    }
}
