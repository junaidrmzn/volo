{
    "name": "@voloiq/mission-management",
    "version": "1.0.0",
    "main": "src/index.tsx",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.development.ts",
        "start:withMocks": "cross-env NODE_ENV=development WITH_MOCKS=true webpack serve --config ./config/webpack.development.ts",
        "start:module": "cross-env NODE_ENV=development webpack serve --config ./config/webpackModuleDevelopment.config.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpackModuleProduction.config.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage ./src/api",
        "lint": "eslint \"{src,config,cypress,test-fixtures}/**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts --passWithNoTests",
        "test:pact": "yarn test:jest 'pacttest'",
        "test": "yarn test:jest --testPathIgnorePatterns '.*\\.pacttest\\..*' && yarn cypress:run",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "cypress:open": "cross-env NODE_ENV=testing cypress open --config-file config/cypress.config.ts",
        "cypress:open:component": "cross-env NODE_ENV=testing cypress open --component --config-file config/cypress.config.ts",
        "cypress:run": " cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts",
        "cypress:run:component": "cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts",
        "cypress:verify": "cypress verify"
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
