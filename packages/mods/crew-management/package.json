{
    "name": "@voloiq/crew-management",
    "version": "1.0.0",
    "main": "src/index.tsx",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.development.ts",
        "start:withMocks": "cross-env NODE_ENV=development WITH_MOCKS=true webpack serve --config ./config/webpack.development.ts",
        "start:module": "cross-env NODE_ENV=development webpack serve --config ./config/webpackModuleDevelopment.config.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpackModuleProduction.config.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage ./src/api",
        "test:pact": "yarn test:jest 'pacttest'",
        "test": "yarn cypress:run",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "lint": "eslint \"{src,config,cypress,test-fixtures}/**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "cypress:open": "cross-env NODE_ENV=testing cypress open --component --config-file cypress.config.ts",
        "cypress:run": "cross-env NODE_ENV=testing cypress run --component --config-file cypress.config.ts",
        "cypress:verify": "cypress verify"
    },
    "dependencies": {
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@mswjs/data": "0.8.4",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/auth": "1.0.0",
        "@voloiq/card-list-item": "1.0.0",
        "@voloiq/crew-management-components": "1.0.0",
        "@voloiq/crew-scheduler": "1.0.0",
        "@voloiq/dates": "1.0.0",
        "@voloiq/feature-flags": "1.0.0",
        "@voloiq/filter-panel": "1.0.0",
        "@voloiq/form": "1.0.0",
        "@voloiq/i18n": "1.0.0",
        "@voloiq/resource-overview": "1.0.0",
        "@voloiq/resource-overview-cypress-page-objects": "1.0.0",
        "@voloiq/routing": "1.0.0",
        "@voloiq/service": "1.0.0",
        "@voloiq/sorting-panel": "1.0.0",
        "@voloiq/table": "1.0.0",
        "@voloiq/text-layouts": "1.0.0",
        "@voloiq/time-scheduler": "1.0.0",
        "@voloiq/utils": "1.0.0",
        "framer-motion": "5.6.0",
        "lodash": "4.17.21",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-use": "17.3.2",
        "ts-pattern": "4.2.1"
    },
    "devDependencies": {
        "@mswjs/data": "0.8.4",
        "@pact-foundation/pact": "9.17.3",
        "@storybook/react": "6.5.9",
        "@testing-library/cypress": "9.0.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react-hooks": "8.0.1",
        "@types/faker": "5.5.3",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@voloiq-typescript-api/crew-api-types": "3.2.15",
        "@voloiq-typescript-api/vertiport-management-types": "4.6.11",
        "@voloiq/testing": "1.0.0",
        "cross-env": "7.0.3",
        "cypress": "12.3.0",
        "date-fns": "2.28.0",
        "eslint": "7.32.0",
        "faker": "5.5.3",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "jest-pact": "0.9.4",
        "msw": "0.36.8",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "ts-node": "10.8.2",
        "typescript": "4.5.5",
        "webpack": "5.68.0",
