{
    "name": "@voloiq/flight-planning",
    "version": "1.0.0",
    "main": "src/index.tsx",
    "scripts": {
        "start:module": "cross-env NODE_ENV=development webpack serve --config ./config/webpackModuleDevelopment.config.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpackModuleProduction.config.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage ./src/api",
        "lint": "eslint \"{src,config,cypress}/**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "publish": "npx pact-broker publish ./pact/pacts --consumer-app-version=1.0.0 --broker-base-url=http://localhost:9292",
        "test": "yarn test:jest --testPathIgnorePatterns '.*\\.pacttest\\..*' && yarn cypress:run:component",
        "test:pact": "yarn test:jest 'pacttest'",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "cypress:open": "cross-env NODE_ENV=testing cypress open --config-file config/cypress.config.ts",
        "cypress:open:component": "cross-env NODE_ENV=testing cypress open --component  --config-file config/cypress.config.ts",
        "cypress:run": "cross-env NODE_ENV=testing cypress run --config-file config/cypress.config.ts",
        "cypress:run:component": "cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts",
        "cypress:verify": "cypress verify",
        "architecture": "depcruise --include-only \"src\" --output-type dot src | dot -T svg > dependencygraph.svg"
    },
    "dependencies": {
        "@azure/abort-controller": "1.1.0",
        "@azure/storage-blob": "12.12.0",
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@mswjs/data": "0.10.1",
        "@testing-library/cypress": "9.0.0",
        "@turf/turf": "6.5.0",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/auth": "1.0.0",
        "@voloiq/card-list-item": "1.0.0",
        "@voloiq/feature-flags": "1.0.0",
