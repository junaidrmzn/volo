{
    "name": "@voloiq/flight-test-instrumentation",
    "version": "1.0.0",
    "main": "src/index.tsx",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.development.ts",
        "start:withMocks": "cross-env NODE_ENV=development WITH_MOCKS=true webpack serve --config ./config/webpack.development.ts",
        "start:module": "cross-env NODE_ENV=development webpack serve --config ./config/webpackModuleDevelopment.config.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpackModuleProduction.config.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage ./src/api",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "typecheck": "tsc --noEmit",
        "cypress:open": "cypress open --config-file config/cypress.config.ts",
        "cypress:open:component": "cross-env NODE_ENV=testing cypress open --component --config-file config/cypress.config.ts",
        "cypress:run": "cypress run --config-file config/cypress.config.ts",
        "cypress:run:component": "cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts",
        "cypress:verify": "cypress verify",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts --passWithNoTests",
        "test": "yarn test:jest && yarn cypress:run:component"
    },
    "dependencies": {
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@tanstack/react-query": "4.26.1",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq-typescript-api/fti-types": "11.0.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/auth": "1.0.0",
        "@voloiq/card-list-item": "1.0.0",
        "@voloiq/date-time-input": "1.0.0",
        "@voloiq/dates": "1.0.0",
        "@voloiq/error-views": "1.0.0",
        "@voloiq/feature-flags": "1.0.0",
        "@voloiq/filter-panel": "1.0.0",
        "@voloiq/form": "1.0.0",
        "@voloiq/i18n": "1.0.0",
        "@voloiq/resource-overview": "1.0.0",
        "@voloiq/resource-overview-cypress-page-objects": "1.0.0",
        "@voloiq/service": "1.0.0",
        "@voloiq/table": "1.0.0",
        "@voloiq/text-layouts": "1.0.0",
        "@voloiq/utils": "1.0.0",
        "framer-motion": "5.6.0",
        "lodash": "4.17.21",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "ts-pattern": "4.0.5"
    },
    "devDependencies": {
        "@pact-foundation/pact": "9.17.3",
        "@storybook/react": "6.5.9",
        "@testing-library/cypress": "9.0.0",
        "@types/lodash": "4.14.191",
        "@types/pg": "8.6.5",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@voloiq/routing": "1.0.0",
        "@voloiq/testing": "1.0.0",
        "cross-env": "7.0.3",
        "cypress": "12.3.0",
        "cypress-file-upload": "5.0.8",
        "dotenv": "16.0.0",
        "eslint": "7.32.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "jest-pact": "0.9.4",
        "jsdom-testing-mocks": "1.6.0",
        "msw": "0.36.8",
        "pg": "8.7.3",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "ts-node": "10.9.1",
        "typescript": "4.5.5",
        "uuidjs": "5.0.0",
        "webpack": "5.68.0",
        "webpack-cli": "5.1.4"
    }
}
