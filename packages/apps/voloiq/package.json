{
    "name": "@voloiq/app",
    "version": "1.0.0",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpackDevelopment.config.ts",
        "start:withMocks": "cross-env NODE_ENV=development WITH_MOCKS=true webpack serve --config ./config/webpackDevelopment.config.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpackProduction.config.ts",
        "clean": "rimraf ./dist ./build-statistics ./coverage",
        "test": "yarn test:jest",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "cypress:open": "cross-env NODE_ENV=testing cypress open --component --config-file config/cypress.config.ts",
        "cypress:run": "cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts"
    },
    "dependencies": {
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@fontsource/montserrat": "4.5.12",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq/aircraft-management": "1.0.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/auth": "1.0.0",
        "@voloiq/battery-management": "1.0.0",
        "@voloiq/booking-management": "1.0.0",
        "@voloiq/commercial-scheduling": "1.0.0",
        "@voloiq/crew-management": "1.0.0",
        "@voloiq/error-views": "1.0.0",
        "@voloiq/feature-flags": "1.0.0",
        "@voloiq/federated-components": "1.0.0",
        "@voloiq/flight-monitoring": "1.0.0",
        "@voloiq/flight-test-definition": "1.0.0",
        "@voloiq/flight-test-instrumentation": "1.0.0",
        "@voloiq/i18n": "1.0.0",
        "@voloiq/logbook": "1.0.0",
        "@voloiq/mission-management": "1.0.0",
        "@voloiq/network-scheduling": "1.0.0",
        "@voloiq/notification-provider": "1.0.0",
        "@voloiq/routing": "1.0.0",
        "@voloiq/service": "1.0.0",
        "@voloiq/utils": "1.0.0",
        "@voloiq/vertiport-management": "1.0.0",
        "date-fns": "2.29.1",
        "framer-motion": "5.6.0",
        "react": "17.0.2",
        "react-countdown-hook": "1.1.1",
        "react-dom": "17.0.2",
        "ts-pattern": "4.2.1"
    },
    "devDependencies": {
        "@jest/types": "27.4.2",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
        "@storybook/react": "6.4.18",
        "@testing-library/cypress": "10.0.1",
        "@testing-library/jest-dom": "5.16.5",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@voloiq/testing": "1.0.0",
        "cross-env": "7.0.3",
        "cypress": "12.3.0",
        "eslint": "7.32.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "msw": "0.36.8",
        "react-refresh": "0.14.0",
        "rimraf": "3.0.2",
