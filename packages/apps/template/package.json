{
    "name": "@voloiq/app-template",
    "version": "1.0.0",
    "main": "src/index.ts",
    "description": "The base template for VoloIq-apps.",
    "scripts": {
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.development.ts",
        "build": "cross-env NODE_ENV=production webpack-cli --color --config ./config/webpack.production.ts"
    },
    "dependencies": {
        "@chakra-ui/react": "1.8.9",
        "@emotion/react": "11.7.1",
        "@emotion/styled": "11.6.0",
        "@volocopter/design-library-react": "42.2.0",
        "@voloiq/app-shell": "1.0.0",
        "@voloiq/routing": "1.0.0",
        "framer-motion": "5.6.0",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "12.1.5",
        "@testing-library/user-event": "13.5.0",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@voloiq/testing": "1.0.0",
        "cross-env": "7.0.3",
        "eslint": "7.32.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "rimraf": "3.0.2",
        "typescript": "4.5.5",
        "webpack": "5.68.0",
        "webpack-cli": "5.1.4"
    }
}
