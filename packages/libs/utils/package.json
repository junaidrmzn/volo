    "main": "dist/voloiq-utils.cjs.js",
    "module": "dist/voloiq-utils.esm.js",
    "types": "dist/voloiq-utils.cjs.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "description": "TypeScript and React utilities",
    "license": "UNLICENSED",
    "author": "Milena Neumann <milena.neumann@volocopter.com>",
    "contributors": [
        "Andre Clausen <andre.clausen@lhind.dlh.de>",
        "Calvin Bayer, <calvin.bayer@volocopter.com>",
