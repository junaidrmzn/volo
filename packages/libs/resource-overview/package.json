        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "test": "yarn test:jest && yarn cypress:run",
        "test:jest": "cross-env NODE_ENV=testing jest --runInBand --config ./config/jest.config.ts --passWithNoTests",
