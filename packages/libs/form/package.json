    "main": "src/index.ts",
    "scripts": {
        "clean": "rimraf ./dist ./build-statistics ./coverage",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "yarn lint --fix",
        "typecheck": "tsc --noEmit",
        "test:jest": "cross-env NODE_ENV=testing jest --config ./config/jest.config.ts",
        "test": "yarn test:jest & yarn cypress:run",
        "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ../../../node_modules/jest/bin/jest.js --runInBand --config ./config/jest.config.ts",
        "cypress:open": "cross-env NODE_ENV=testing cypress open --component --config-file config/cypress.config.ts",
        "cypress:run": "cross-env NODE_ENV=testing cypress run --component --config-file config/cypress.config.ts"
    },
